import streamlit as st
import pandas as pd

# –ü–ª–∞—Ç—Ñ–æ—Ä–º–∞ –±–∞–ø—Ç–∞—É–ª–∞—Ä—ã
st.set_page_config(page_title="AI Organic Lab 34", layout="wide")

# 34 –ó–µ—Ä—Ç—Ö–∞–Ω–∞–ª—ã“õ –∂“±–º—ã—Å –¥–µ—Ä–µ–∫—Ç–µ—Ä –±–∞–∑–∞—Å—ã
lab_data = {
    "1. –ú–µ—Ç–∞–Ω–Ω—ã“£ —Ö–ª–æ—Ä–ª–∞–Ω—É—ã": {"reagents": ["CH4", "Cl2"], "condition": "hv (–∂–∞—Ä—ã“õ)", "effect": "üí® HCl –≥–∞–∑—ã –±”©–ª—ñ–Ω–¥—ñ, —Ç“Ø—Å—Å—ñ–∑–¥–µ–Ω—É", "equation": "CH4 + Cl2 -> CH3Cl + HCl"},
    "6. –≠—Ç–∏–ª–µ–Ω–Ω—ñ“£ –±—Ä–æ–º–¥–∞–ª—É—ã": {"reagents": ["C2H4", "Br2"], "condition": "—Å—Ç–∞–Ω–¥–∞—Ä—Ç—Ç—ã", "effect": "üü† “ö–æ“£—ã—Ä —Ç“Ø—Å—Ç—ñ –±—Ä–æ–º —Å—É—ã —Ç“Ø—Å—Å—ñ–∑–¥–µ–Ω–¥—ñ", "equation": "C2H4 + Br2 -> C2H4Br2"},
    "11. –ê—Ü–µ—Ç–∏–ª–µ–Ω –∞–ª—É": {"reagents": ["CaC2", "H2O"], "condition": "—Å—Ç–∞–Ω–¥–∞—Ä—Ç—Ç—ã", "effect": "ü´ß –ì–∞–∑ “õ–∞—Ä“õ—ã–Ω–¥—ã –±”©–ª—ñ–Ω—É—ñ", "equation": "CaC2 + 2H2O -> C2H2 + Ca(OH)2"},
    "21. –ì–ª–∏—Ü–µ—Ä–∏–Ω–≥–µ —Å–∞–ø–∞–ª—ã“õ —Ä–µ–∞–∫—Ü–∏—è": {"reagents": ["Glycerol", "Cu(OH)2"], "condition": "—Å—ñ–ª—Ç—ñ–ª—ñ", "effect": "üü¶ –ê—à—ã“õ –∫”©–∫ —Ç“Ø—Å—Ç—ñ –µ—Ä—ñ—Ç—ñ–Ω–¥—ñ —Ç“Ø–∑—ñ–ª–¥—ñ", "equation": "–ì–ª–∏—Ü–µ—Ä–∞—Ç –º–µ–¥–∏ —Ç“Ø–∑—ñ–ª—É—ñ"},
    "26. –ö“Ø–º—ñ—Å –∞–π–Ω–∞ —Ä–µ–∞–∫—Ü–∏—è—Å—ã": {"reagents": ["CH3CHO", "AgNO3", "NH3"], "condition": "t (“õ—ã–∑–¥—ã—Ä—É)", "effect": "ü™û –ü—Ä–æ–±–∏—Ä–∫–∞ “õ–∞–±—ã—Ä“ì–∞—Å—ã–Ω–¥–∞ –∫“Ø–º—ñ—Å “õ–∞–±–∞—Ç—ã", "equation": "CH3CHO + 2[Ag(NH3)2]OH -> ... + 2Ag"},
    "31. –≠—Ç–µ—Ä–∏—Ñ–∏–∫–∞—Ü–∏—è (–≠—Ñ–∏—Ä –∞–ª—É)": {"reagents": ["CH3COOH", "C2H5OH"], "condition": "H2SO4", "effect": "üçé –ñ–∞“ì—ã–º–¥—ã –∂–µ–º—ñ—Å –∏—ñ—Å—ñ —à—ã“õ—Ç—ã", "equation": "CH3COOC2H5 + H2O"}
}

# –ò–Ω—Ç–µ—Ä—Ñ–µ–π—Å
st.title("üß™ AI Virtual Organic Lab: 34 –ó–µ—Ä—Ç—Ö–∞–Ω–∞–ª—ã“õ –¢–∞–ø—Å—ã—Ä–º–∞")
st.markdown("---")

# –ú“±“ì–∞–ª—ñ–º –º–µ–Ω –û“õ—É—à—ã“ì–∞ –∞—Ä–Ω–∞–ª“ì–∞–Ω –º–µ–Ω—é
choice = st.sidebar.selectbox("–ó–µ—Ä—Ç—Ö–∞–Ω–∞–ª—ã“õ –∂“±–º—ã—Å—Ç—ã —Ç–∞“£–¥–∞“£—ã–∑:", list(lab_data.keys()))

col1, col2 = st.columns([2, 1])

with col1:
    st.header("üî¨ –≠–∫—Å–ø–µ—Ä–∏–º–µ–Ω—Ç –∞–ª–∞“£—ã")
    st.info(f"–¢–∞–ø—Å—ã—Ä–º–∞: {choice} –±–æ–π—ã–Ω—à–∞ —Ç”ô–∂—ñ—Ä–∏–±–µ –∂–∞—Å–∞“£—ã–∑.")
    
    # –†–µ–∞–∫—Ü–∏—è –ø–∞—Ä–∞–º–µ—Ç—Ä–ª–µ—Ä—ñ
    user_reagents = st.multiselect("–†–µ–∞–≥–µ–Ω—Ç—Ç–µ—Ä–¥—ñ —Ç–∞“£–¥–∞“£—ã–∑:", ["CH4", "Cl2", "C2H4", "Br2", "CaC2", "H2O", "Cu(OH)2", "CH3CHO", "AgNO3", "NH3", "CH3COOH", "C2H5OH"])
    user_cond = st.radio("–†–µ–∞–∫—Ü–∏—è –∂–∞“ì–¥–∞–π—ã:", ["—Å—Ç–∞–Ω–¥–∞—Ä—Ç—Ç—ã", "hv (–∂–∞—Ä—ã“õ)", "t (“õ—ã–∑–¥—ã—Ä—É)", "H2SO4 (–∫–∞—Ç)"])
    
    if st.button("üß™ –†–µ–∞–∫—Ü–∏—è–Ω—ã —ñ—Å–∫–µ “õ–æ—Å—É"):
        data = lab_data[choice]
        if set(data["reagents"]).issubset(set(user_reagents)) and user_cond in data["condition"]:
            st.success(f"–ù”ô—Ç–∏–∂–µ: {data['effect']}")
            st.snow()
        else:
            st.error("–†–µ–∞–∫—Ü–∏—è –∂“Ø—Ä–º–µ–¥—ñ! –†–µ–∞–≥–µ–Ω—Ç—Ç–µ—Ä–¥—ñ –Ω–µ–º–µ—Å–µ –∂–∞“ì–¥–∞–π–¥—ã (hv, t, –∫–∞—Ç) –¥“±—Ä—ã—Å —Ç–∞“£–¥–∞–º–∞–¥—ã“£—ã–∑.")

with col2:
    st.header("ü§ñ AI –¢–µ–∫—Å–µ—Ä—É—à—ñ")
    st.write("–†–µ–∞–∫—Ü–∏—è —Ç–µ“£–¥–µ—É—ñ–Ω –∂–∞–∑—ã“£—ã–∑:")
    user_eq = st.text_input("–¢–µ“£–¥–µ—É (–º—ã—Å–∞–ª—ã: C2H4 + Br2 -> C2H4Br2)")
    
    if st.button("üìù –¢–µ–∫—Å–µ—Ä—É"):
        # AI-–¥—ã“£ “õ–∞—Ä–∞–ø–∞–π—ã–º —Ç–µ–∫—Å–µ—Ä—É –∞–ª–≥–æ—Ä–∏—Ç–º—ñ
        if lab_data[choice]["equation"].split(" -> ")[1] in user_eq:
            st.success("AI: –¢–µ“£–¥–µ—É –¥“±—Ä—ã—Å! ”®–Ω—ñ–º –¥“±—Ä—ã—Å –∞–Ω—ã“õ—Ç–∞–ª“ì–∞–Ω. 5/5")
        else:
            st.warning("AI: “ö–∞—Ç–µ –±–∞—Ä –Ω–µ–º–µ—Å–µ ”©–Ω—ñ–º –¥“±—Ä—ã—Å –µ–º–µ—Å. –û“õ—É–ª—ã“õ—Ç—ã “õ–∞—Ä–∞“£—ã–∑.")

st.markdown("---")
st.caption("–ú“±“ì–∞–ª—ñ–º–¥–µ—Ä –º–µ–Ω –æ“õ—É—à—ã–ª–∞—Ä“ì–∞ –∞—Ä–Ω–∞–ª“ì–∞–Ω –æ–Ω–ª–∞–π–Ω —Ö–∏–º–∏—è–ª—ã“õ –ø–ª–∞—Ç—Ñ–æ—Ä–º–∞ ¬©Ô∏è 2024")
